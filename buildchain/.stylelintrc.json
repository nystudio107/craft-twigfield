{
  "extends": [
    "stylelint-config-recommended",
    "stylelint-config-standard-scss"
  ],
  "rules": {
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "screen",
          "extends",
          "responsive",
          "tailwind"
        ]
      }
    ],
    "block-no-empty": null,
    "max-line-length": null
  }
}
